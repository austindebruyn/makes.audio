{
    "extends": "standard",
    "rules": {
      "comma-dangle": ["error", "never"],
      "space-before-function-paren": ["error", {
        "anonymous": "always",
        "named": "never"
      }],
      "no-multi-spaces": "off",
      "no-return-assign": "off",
      "semi": ["error", "never"]
    }
}
